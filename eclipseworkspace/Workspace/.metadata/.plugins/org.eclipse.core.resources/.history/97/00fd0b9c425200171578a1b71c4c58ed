package com.wang.priactice;

public class TwoTree {
	public static void main(String[] args) {
		// 二叉树
		int[] array = {1,2,3,4,5,6,7};
		int parent = 4;
		System.out.println("         "+parent);
		for (int i : array) {
			if(i<parent){
				System.out.print("      "+i);
				parent=i;
			}
			for(int j: array){
				if(j>parent){
					System.out.print("      "+j);
					break;
				}
			}
			System.out.println();
		}
	}
}
